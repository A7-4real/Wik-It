{
  "manifest_version": 2,
  "name": "Wik-It",
  "version": "1.0",
  "description": "search Wikipedia on the go!",

  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },

  "background": {
    "scripts": ["eventpage.js"],
    "persistent": false
  },

  "permissions": ["tabs", "contextMenus"]
}
